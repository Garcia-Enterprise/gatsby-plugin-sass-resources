{
  "name": "@garcia-enterprise/gatsby-plugin-sass-resources",
  "packageManager": "yarn@3.6.0",
  "version": "4.0.0",
  "author": "Eric N. Garcia <eng618@garciaericn.com>",
  "repository": "git@github.com:Garcia-Enterprise/gatsby-plugin-sass-resources.git",
  "bugs": {
    "url": "https://github.com/Garcia-Enterprise/gatsby-plugin-sass-resources/issues"
  },
  "description": "SASS resources (e.g. variables, mixins etc.) plugin for Gatsby",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir .",
    "prepublish": "NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir .",
    "release": "auto shipit"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "sass-resources-loader": "^2.2.5",
    "webpack": "^5.87.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^10.46.0",
    "@auto-it/conventional-commits": "^10.46.0",
    "@auto-it/twitter": "^10.46.0",
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "auto": "^10.46.0"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0"
  },
  "readme": "README.md",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "sass",
    "scss",
    "sass-resources",
    "scss-resources"
  ],
  "auto": {
    "plugins": [
      [
        "twitter",
        {
          "threshold": "patch"
        }
      ],
      "npm",
      "all-contributors",
      "conventional-commits",
      "released"
    ]
  }
}
